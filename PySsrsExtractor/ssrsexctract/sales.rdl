        <CommandText>--declare		@Columns nvarchar(max)
--set @Columns= 'p.EnglishProductName, p.ProductLine'

Drop table if exists ##temp
declare @sql nvarchar(max)
SET @sql =
N'SELECT * into ##temp from (
select
     '+  @Columns +'
	,SalesAmount= sum(s.[SalesAmount])
	,TaxAmt     = sum([TaxAmt])
	, Freight   = sum([Freight])   
			 
  FROM [AdventureWorksDW2017].[dbo].[FactInternetSales] s

  JOIN  [AdventureWorksDW2017].[dbo].DimProduct p
  ON    p.ProductKey = s.ProductKey
  GROUP BY ' + @Columns +'
 )a';
 
 PRINT @SQL;
EXEC sp_executesql @SQL;

select * from ##temp

Drop table if exists ##temp</CommandText>
      </Query>
